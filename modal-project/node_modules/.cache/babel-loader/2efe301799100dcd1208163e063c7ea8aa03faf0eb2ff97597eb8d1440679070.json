odal: false,\n      showModalTwo: false,\n    };\n  },\n  methods: {\n    handleClick() {\n      console.log(this.$refs.name);\n      this.$refs.name.classList.add(\"active\");\n      this.$refs.name.focus();\n    },\n    toogleModal() {\n      this.showModal = !this.showModal;\n    },\n    toggleModalTwo() {\n      this.showModalTwo = !this.showModalTwo;\n    },\n  },\n  components: {\n    ModalComp,\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n\nh1 {\n  border-bottom: 1px solid #ddd;\n  display: inline-block;\n  padding-bottom: 10px;\n}\n</style>\n"],"mappings":"AA0BA,OAAOA,SAAQ,MAAO,4BAA4B;AAElD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,qBAAqB;MAC5BC,SAAS,EAAE,KAAK;MAChBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,KAAK,CAACT,IAAI,CAAC;MAC5B,IAAI,CAACS,KAAK,CAACT,IAAI,CAACU,SAAS,CAACC,GAAG,CAAC,QAAQ,CAAC;MACvC,IAAI,CAACF,KAAK,CAACT,IAAI,CAACY,KAAK,CAAC,CAAC;IACzB,CAAC;IACDC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACV,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAS;IAClC,CAAC;IACDW,cAAcA,CAAA,EAAG;MACf,IAAI,CAACV,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC;EACF,CAAC;EACDW,UAAU,EAAE;IACVhB;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}